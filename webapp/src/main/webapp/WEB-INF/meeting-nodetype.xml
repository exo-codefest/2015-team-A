<nodeTypes
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:mix="http://www.jcp.org/jcr/mix/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:exo="http://www.exoplatform.com/jcr/exo/1.0">

  <nodeType name="exo:meeting" isMixin="false" hasOrderableChildNodes="false" primaryItemName=""  >

    <supertypes>
      <supertype>nt:base</supertype>
    </supertypes>

    <propertyDefinitions>
      <propertyDefinition name="exo:meetingTitle" requiredType="String" autoCreated="false" mandatory="false"
                          onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>

      <propertyDefinition name="exo:meetingValidation" requiredType="Long" autoCreated="false" mandatory="false"
                          onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>

      <propertyDefinition name="exo:meetingDescription" requiredType="String" autoCreated="false" mandatory="false"
                          onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>

    </propertyDefinitions>

    <childNodeDefinitions>
      <childNodeDefinition name="comments" defaultPrimaryType="" autoCreated="false" mandatory="false"
                           onParentVersion="COPY" protected="false" sameNameSiblings="false">
        <requiredPrimaryTypes>
          <requiredPrimaryType>nt:unstructured</requiredPrimaryType>
        </requiredPrimaryTypes>
      </childNodeDefinition>
    </childNodeDefinitions>
  </nodeType>

  <nodeType name="mix:timeOption" isMixin="true" hasOrderableChildNodes="false" primaryItemName="">
    <propertyDefinitions>

      <propertyDefinition name="exo:timeOptionId" requiredType="Integer" autoCreated="false" mandatory="false"
                          onParentVersion="COPY" protected="false" multiple="false" >
        <valueConstraints/>
      </propertyDefinition>

      <propertyDefinition name="exo:timeOptionFromDate" requiredType="Long" autoCreated="false" mandatory="false"
                          onParentVersion="COPY" protected="false" multiple="false" >
        <valueConstraints/>
      </propertyDefinition>

      <propertyDefinition name="exo:timeOptionToDate" requiredType="Long" autoCreated="false" mandatory="false"
                          onParentVersion="COPY" protected="false" multiple="false" >
        <valueConstraints/>
      </propertyDefinition>

    </propertyDefinitions>
  </nodeType>


</nodeTypes>